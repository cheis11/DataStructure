package week12;

public class Exercise2 {

	public static void main(String[] args) {
		int[] arr = {2, 6, 8, 14};
		int a, b, temp = 1;
        b = arr[0];
        for(int i = 1; i < arr.length; i++){
            a = arr[i];
            temp = a % b;
            while(temp != 0){
                a = b;
                b = temp;
                temp = a % b;
            }
        }
        System.out.println(b);
	}

}
